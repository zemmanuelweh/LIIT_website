{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course",
    "description": "Individual courses within programs"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "code": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "credits": {
      "type": "integer",
      "required": true,
      "min": 1,
      "max": 6
    },
    "instructor": {
      "type": "string",
      "required": true
    },
    "program": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::program.program"
    },
    "semester": {
      "type": "enumeration",
      "enum": [
        "Fall",
        "Spring",
        "Summer"
      ],
      "required": true
    },
    "year": {
      "type": "integer",
      "required": true
    }
  }
}